// WARNING
//
// Do NOT edit this file while ZNC is running!
// Use webadmin or *controlpanel instead.
//
// Altering this file by hand will forfeit all support.
//
// But if you feel risky, you might want to read help on /znc saveconfig and /znc rehash.
// Also check http://en.znc.in/wiki/Configuration

AnonIPLimit = 10
ConnectDelay = 5
LoadModule = webadmin
MaxBufferSize = 500
ProtectWebSessions = true
SSLCertFile = /home/znc-user/.znc/znc.pem
ServerThrottle = 30
Version = 1.4

<Listener listener0>
        AllowIRC = true
        AllowWeb = true
        IPv4 = true
        IPv6 = false
        Port = 6697
        SSL = false
</Listener>

<User kedo>
        Admin = true
        Allow = *
        AltNick = kedo_
        AppendTimestamp = false
        AutoClearChanBuffer = false
        Buffer = 100
        ChanModes = +stn
        DenyLoadMod = false
        DenySetBindHost = false
        Ident = kedo
        JoinTries = 10
        LoadModule = chansaver
        LoadModule = log
        MaxJoins = 0
        MaxNetworks = 1
        MultiClients = true
        Nick = kedo
        PrependTimestamp = true
        QuitMsg = (´・ω・｀)
        RealName = Mr. Kedo
        StatusPrefix = *
        TimestampFormat = [%H:%M:%S]

        <Network rizon>
                FloodBurst = 4
                FloodRate = 1.00
                IRCConnectEnabled = true
                LoadModule = chansaver
                LoadModule = nickserv
                LoadModule = simple_away
                Server = irc.rizon.net +6697

                <Chan #CAA>
                </Chan>
        </Network>

        <Pass password>
                Hash = d01ab009eaca50929f7d46a1598788fee2f32cc8452cf3eced4cec1f9c117f91
                Method = SHA256
                Salt = yE3)dw1anU6wRDQH-wxv
        </Pass>
</User>
